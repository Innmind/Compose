{
    "name": "innmind/compose",
    "type": "library",
    "description": "Service locator",
    "keywords": ["service", "locator", "dependency", "injection"],
    "homepage": "http://github.com/Innmind/Compose",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Compose/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/immutable": "~2.7",
        "symfony/yaml": "~4.0",
        "psr/container": "^1.0",
        "symfony/options-resolver": "^4.0",
        "innmind/url": "^2.0",
        "innmind/url-resolver": "^3.0",
        "symfony/config": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Compose\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Compose\\": "tests/",
            "Fixture\\Innmind\\Compose\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "giorgiosironi/eris": "^0.9.0",
        "innmind/graphviz": "^1.0",
        "mnapoli/silly": "^1.7",
        "innmind/server-control": "^2.1",
        "innmind/filesystem": "^3.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "suggest": {
        "innmind/graphviz": "To visualize your dependency graph"
    },
    "bin": ["bin/compose"]
}
